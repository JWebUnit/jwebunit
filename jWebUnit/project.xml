    <build>
        <sourceDirectory>src</sourceDirectory>
        <unitTestSourceDirectory>test</unitTestSourceDirectory>
		<unitTest>
		    <includes>
		        <include>**/*Test.java</include>
		    </includes>
			<excludes>
				<exclude>**/JWebUnitTest.java</exclude>
			</excludes>
			<resources>
				<resource>
					<directory>${basedir}/test</directory>
					<includes>
						<include>*.xml</include>
					</includes>
				</resource>
			</resources>
		</unitTest>
    </build>
